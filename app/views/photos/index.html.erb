<header class="masthead">
  <div class="container">


    <div class="page-header">
      <h1>
        <span class="glyphicon glyphicon-camera" aria-hidden="true"></span>
        My Photos
      </h1>
    </div>
    <div class="row">
      <% if current_user.photos.blank? %>
        <% 30.times do %>
          <div class="col-xs-4 col-md-3">
            <img src="http://static2.bigstockphoto.com/thumbs/1/3/1/large1500/13144754.jpg" alt="picture" class="my-photo img-thumbnail">
          <% end %>
        <% else %>
          <% @photos.each do |photo| %>
            <%= photo.image %>
          <% end %>
        </div>
      <% end %>
    </div>
  </header>
